{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    
    "rules": {
        "eqeqeq":"error",
        "no-trailing-spaces": "error",
        "object-curly-spacing": [
            "error", "always"
        ],
        "arrow-spacing": [
            "error", { "before": true, "after": true }
        ],
        "indent": [
            "error",
            2
        ],
        "quotes": [2, "double",{"avoidEscape": true}],
        "linebreak-style": ["error", "windows"],
        "no-console": 0
    }
}
